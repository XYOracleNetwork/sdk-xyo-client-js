{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.72.9",
  "description": "XYO Node App",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.408.0",
    "@xylabs/assert": "^2.10.0",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/account": "workspace:~",
    "@xyo-network/account-model": "workspace:~",
    "@xyo-network/express-node-server": "workspace:~",
    "@xyo-network/node": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/payload-validator": "workspace:~",
    "@xyo-network/schema-name-validator": "workspace:~",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.19.5",
    "@xylabs/tsconfig": "^2.19.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "types": "dist/index.d.ts",
  "scripts": {
    "package-compile": "tsup && package-publint",
    "package-recompile": "tsup && package-publint"
  }
}
