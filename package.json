{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/api": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/domain": "workspace:^",
    "@xyo-network/network": "workspace:^",
    "@xyo-network/storage": "workspace:^",
    "@xyo-network/utils": "workspace:^",
    "@xyo-network/witnesses": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "license-checker": "^25.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "scripts": {
    "build-all": "yarn workspaces foreach -ptR run build",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn cycle && yarn workspaces foreach run build && yarn version apply --all && yarn workspaces foreach npm publish",
    "cycle": "yarn workspaces foreach run cycle",
    "clean-all": "yarn workspaces foreach run clean",
    "deps-all": "yarn workspaces foreach run deps",
    "fix": "yarn workspaces foreach run fix",
    "test": "yarn jest .",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": true,
  "types": "dist/esm/index.d.ts",
  "version": "2.20.23",
  "packageManager": "yarn@3.2.0"
}
