{
  "name": "@xyo-network/sdk-xyo-client-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.js",
  "sideEffects": false,
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.0.9",
    "sha.js": "^2.4.11"
  },
  "scripts": {
    "build": "yarn build:ci",
    "build:ci": "yarn lint && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn publish --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha -r ts-node/register 'src/**/*.spec.ts'",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "eslint-plugin-workspaces": "^0.6.2",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rollup": "^2.42.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "ts-prune": "^0.9.0",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  }
}