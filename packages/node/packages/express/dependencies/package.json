{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-dependencies",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.42.11",
  "description": "XYO Express Node Dependencies",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/assert": "^2.6.11",
    "@xylabs/sdk-api-express-ecs": "^1.2.5",
    "@xyo-network/express-node-lib": "workspace:^",
    "@xyo-network/express-node-middleware": "workspace:^",
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/node-core-modules-memory": "workspace:^",
    "@xyo-network/node-core-modules-mongo": "workspace:^",
    "@xyo-network/node-core-query-handlers": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.9.0"
  },
  "peerDependencies": {
    "axios": "*",
    "pako": "*"
  },
  "types": "dist/esm/index.d.ts"
}
