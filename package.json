{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/api": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/cryptomarket-witness": "workspace:^",
    "@xyo-network/domain": "workspace:^",
    "@xyo-network/ethereum-witness": "workspace:^",
    "@xyo-network/network": "workspace:^",
    "@xyo-network/panel": "workspace:^",
    "@xyo-network/schema-witness": "workspace:^",
    "@xyo-network/storage": "workspace:^",
    "@xyo-network/systeminfo-witness": "workspace:^",
    "@xyo-network/utils": "workspace:^",
    "@xyo-network/witnesses": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "license-checker": "^25.0.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.2"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn gen:docs",
    "build:cjs": "yarn workspaces foreach -ptRv run build:package:cjs",
    "build:esm": "yarn workspaces foreach -ptRv run build:package:esm",
    "gen:docs": "yarn workspaces foreach -pv run gen:docs:package",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn cycle && yarn workspaces foreach run build && yarn version apply --all && yarn workspaces foreach npm publish",
    "gen:docs:package": "typedoc $INIT_CWD/src/index.ts --json $INIT_CWD/dist/docs.json",
    "clean:package": "rimraf -q $INIT_CWD/dist && rimraf -q $INIT_CWD/build",
    "fix:package": "yarn eslint $INIT_CWD/src --fix",
    "lint:package": "yarn eslint $INIT_CWD/src",
    "deps:package": "depcheck $INIT_CWD",
    "updo3:package": "yarn update-interactive",
    "build:package:cjs": "tsc -p $INIT_CWD/tsconfig.build.cjs.json",
    "build:package:esm": "tsc -p $INIT_CWD/tsconfig.build.esm.json",
    "test": "yarn jest .",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": true,
  "types": "dist/esm/index.d.ts",
  "version": "2.20.39",
  "packageManager": "yarn@3.2.0"
}
