{
  "name": "@xyo-network/sdk-xyo-client-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./index.d.js",
  "exports": {
    ".": "./index.esm.js"
  },
  "sideEffects": false,
  "dependencies": {
    "@xyo-network/sdk-xyo-js": "^2.1.8",
    "@xyo-network/sdk-xyo-mongo-js": "^2.1.3",
    "axios": "~0.21.1",
    "mongodb": "^4.0.0-beta.4",
    "noble-ed25519": "~1.2.1",
    "sha.js": "~2.4.11"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn compile && yarn copy-src",
    "copy-src": "cp package.json ./dist && cp -R ./src/* ./dist",
    "compile": "rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "yarn build && yarn jest ./src",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck --ignores \"vue, ts-node, @types/jest\""
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-prune": "^0.9.0",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}