  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/payload-plugins": "workspace:^",
    "@xyo-network/sdk": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@xylabs/eslint-config": "^2.7.23",
    "@xylabs/ts-scripts-yarn3": "^2.7.23",
    "@xylabs/tsconfig": "^2.7.23",
    "axios": "^1.1.3",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "jest-extended": "^3.1.0",
    "pako": "^2.0.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "axios": "^1.1.3",
    "lodash": "^4.17.21",
    "pako": "^2.0.4"
  },
  "resolutions": {
    "@uniswap/v3-periphery": "1.4.1",
    "bn.js": "^5.2.1",
    "lodash": "^4.17.21"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Root Package"
  },
  "scripts": {
    "lint-pkg": "npmPkgJsonLint .",
    "xyo": "yarn workspace @xyo-network/node-cli xyo",
    "xyo-ts": "yarn workspace @xyo-network/node-cli xyo-ts"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.38.8",
  "packageManager": "yarn@3.2.4"
}
