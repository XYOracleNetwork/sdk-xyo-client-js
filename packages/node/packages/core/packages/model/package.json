{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-core-model",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.53.34",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xyo-network/archivist": "workspace:^",
    "@xyo-network/boundwitness-model": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/diviner": "workspace:^",
    "@xyo-network/module": "workspace:^",
    "@xyo-network/module-model": "workspace:^",
    "@xyo-network/payload-model": "workspace:^",
    "@xyo-network/shared": "workspace:^",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.16.1",
    "@xylabs/tsconfig": "^2.16.1",
    "typescript": "^4.9.5"
  },
  "types": "dist/types/index.d.ts"
}
