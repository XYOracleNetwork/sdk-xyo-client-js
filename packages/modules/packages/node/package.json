{
  "name": "@xyo-network/node",
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "dependencies": {
    "@xyo-network/node-abstract": "workspace:~",
    "@xyo-network/node-memory": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/node-wrapper": "workspace:~"
  },
  "devDependencies": {
    "@xylabs/delay": "^2.12.22",
    "@xylabs/ts-scripts-yarn3": "^3.0.88",
    "@xylabs/tsconfig": "^3.0.88",
    "@xyo-network/account": "workspace:~",
    "@xyo-network/account-model": "workspace:~",
    "@xyo-network/address-payload-plugin": "workspace:~",
    "@xyo-network/archivist": "workspace:~",
    "@xyo-network/archivist-model": "workspace:~",
    "@xyo-network/diviner": "workspace:~",
    "@xyo-network/manifest-model": "workspace:~",
    "@xyo-network/module-model": "workspace:~",
    "@xyo-network/payload": "workspace:~",
    "@xyo-network/witnesses": "workspace:~",
    "typescript": "^5.2.2"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "docs": "dist/docs.json",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.js"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "sideEffects": false,
  "version": "2.75.14"
}
