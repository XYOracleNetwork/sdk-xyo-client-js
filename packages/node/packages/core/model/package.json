{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-core-model",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.42.3",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/assert": "^2.6.9",
    "@xyo-network/account": "workspace:^",
    "@xyo-network/api": "workspace:^",
    "@xyo-network/archivist": "workspace:^",
    "@xyo-network/boundwitness": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/diviner": "workspace:^",
    "@xyo-network/module": "workspace:^",
    "@xyo-network/payload": "workspace:^",
    "@xyo-network/shared": "workspace:^",
    "ajv": "^8.11.2"
  },
  "devDependencies": {
    "axios": "~1.2.1",
    "pako": "^2.1.0"
  },
  "peerDependencies": {
    "axios": "~1.2.1",
    "pako": "^2.1.0"
  },
  "types": "dist/esm/index.d.ts"
}
