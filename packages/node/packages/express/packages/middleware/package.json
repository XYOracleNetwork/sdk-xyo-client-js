{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-middleware",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.79.2",
  "description": "XYO Express Node Middleware",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.js"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.449.0",
    "@xylabs/assert": "^2.13.3",
    "@xylabs/sdk-api-express-ecs": "^1.4.0",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/shared": "workspace:~",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/express-mung": "^0.5.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@xylabs/ts-scripts-yarn3": "^3.1.21",
    "@xylabs/tsconfig": "^3.1.21",
    "typescript": "^5.2.2"
  }
}
