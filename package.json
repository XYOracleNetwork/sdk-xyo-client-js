{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/plugins": "workspace:^",
    "@xyo-network/sdk": "workspace:^"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@xylabs/eslint-config": "^2.17.12",
    "@xylabs/ts-scripts-yarn3": "^2.17.12",
    "@xylabs/tsconfig": "^2.17.12",
    "@xylabs/tsconfig-dom-jest": "^2.17.12",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "bn.js": "^5",
    "quick-lru": "^5",
    "typescript": "^4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0",
  "name": "@xyo-network/sdk-xyo-client-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Root Package"
  },
  "scripts": {
    "coverage": "yarn jest --coverage --forceExit",
    "compile": "./scripts/clear-scrollback-buffer.sh && yarn xy compile",
    "build": "xy build && xy statics",
    "deploy": "xy deploy",
    "lint-pkg": "npmPkgJsonLint .",
    "start": "nodemon",
    "perf": "yarn jest -c jest.config.perf.cjs",
    "test": "jest --no-cache --forceExit",
    "test:ci": "jest --runInBand --coverage --forceExit --passWithNoTests",
    "xyo": "node ./packages/cli/dist/cjs/index.js",
    "xyo-ts": "yarn workspace @xyo-network/node-cli xyo-ts",
    "free-8080": "kill -9 $(lsof -t -i :8080)",
    "free-3033": "kill -9 $(lsof -t -i :3033)"
  },
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "version": "2.56.2",
  "packageManager": "yarn@3.5.1",
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  }
}
