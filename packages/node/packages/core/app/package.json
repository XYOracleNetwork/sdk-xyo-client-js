{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.43.0",
  "description": "XYO Node App",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.2.5",
    "@xyo-network/account": "workspace:^",
    "@xyo-network/express-node-server": "workspace:^",
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/node": "workspace:^",
    "axios": "~1.2.1",
    "dotenv": "^16.0.3",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.11.16"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "types": "dist/esm/index.d.ts"
}
