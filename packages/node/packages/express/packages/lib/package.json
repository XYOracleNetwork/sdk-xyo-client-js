{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.63.1",
  "description": "XYO Express Node Lib",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.358.0",
    "@xylabs/assert": "^2.9.1",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/node-model": "workspace:^",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.1",
    "winston": "^3.9.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@xylabs/ts-scripts-yarn3": "^2.17.17",
    "@xylabs/tsconfig": "^2.17.17",
    "typescript": "^5.1.3"
  },
  "types": "dist/types/index.d.ts"
}
