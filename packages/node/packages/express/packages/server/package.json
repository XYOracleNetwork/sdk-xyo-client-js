{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.67.0",
  "description": "XYO Express Node Server",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.377.0",
    "@xylabs/assert": "^2.9.3",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/express-node-dependencies": "workspace:~",
    "@xyo-network/express-node-middleware": "workspace:~",
    "@xyo-network/express-node-routes": "workspace:~",
    "@xyo-network/node": "workspace:~",
    "@xyo-network/node-core-model": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/shared": "workspace:~",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "nconf": "^0.12.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.17",
    "@types/nconf": "^0.10.3",
    "@xylabs/ts-scripts-yarn3": "^2.19.0",
    "@xylabs/tsconfig": "^2.19.0",
    "typescript": "^5.1.6"
  },
  "types": "dist/types/index.d.ts"
}
