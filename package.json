{
  "name": "@xyo-network/sdk-xyo-client-js",
  "version": "5.3.9",
  "private": true,
  "description": "Primary SDK for using XYO Protocol 2.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "git+https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "license": "LGPL-3.0-only",
  "author": {
    "name": "XYO Development Team",
    "email": "support@xyo.network",
    "url": "https://xyo.network"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/neutral/index.d.ts",
      "default": "./dist/neutral/index.mjs"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md"
  },
  "module": "./dist/neutral/index.mjs",
  "types": "./dist/neutral/index.d.ts",
  "files": [
    "dist",
    "src",
    "!**/*.bench.*",
    "!**/*.spec.*",
    "!**/*.test.*"
  ],
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "bench": "vitest bench",
    "build": "xy build -j 8 && xy statics",
    "build-typedoc-site": "typedoc",
    "compile": "./scripts/clear-scrollback-buffer.sh && yarn xy compile",
    "coverage": "vitest --coverage --watch false",
    "cycle": "node ./scripts/run-cycle.mjs",
    "deploy": "xy deploy",
    "free-3033": "kill -9 $(lsof -t -i :3033)",
    "free-8080": "kill -9 $(lsof -t -i :8080)",
    "free-mongo": "kill -9 $(lsof -t -i :55391) && kill -9 $(lsof -t -i :55393)",
    "package-build": "yarn package-clean && rimraf docs && yarn package-build-only && typedoc && xy statics",
    "package-clean": "rimraf dist",
    "publish-all": "npm publish --workspaces",
    "readme:gen": "node scripts/readme-gen.mjs",
    "test": "vitest --watch false"
  },
  "devDependencies": {
    "@firebase/app": "~0.14.8",
    "@firebase/app-compat": "~0.5.8",
    "@stylistic/eslint-plugin": "~5.9.0",
    "@types/node": "^25.3.0",
    "@types/supertest": "~6.0.3",
    "@typescript-eslint/eslint-plugin": "~8.56.1",
    "@typescript-eslint/parser": "~8.56.1",
    "@typescript-eslint/types": "~8.56.1",
    "@vitest/coverage-v8": "~4.0.18",
    "@xylabs/eslint-config-flat": "~7.3.2",
    "@xylabs/ts-scripts-yarn3": "~7.3.2",
    "@xylabs/tsconfig": "~7.3.2",
    "@xylabs/tsconfig-dom": "~7.3.2",
    "@xylabs/vitest-extended": "~5.0.80",
    "@xyo-network/modules": "~5.3.9",
    "@xyo-network/sdk-utils": "~5.3.9",
    "chalk": "~5.6.2",
    "dependency-cruiser": "~17.3.8",
    "dotenv": "~17.3.1",
    "eslint": "~9.39.3",
    "eslint-import-resolver-typescript": "~4.4.4",
    "glob": "~13.0.6",
    "jsdom": "^28.1.0",
    "knip": "~5.85.0",
    "reflect-metadata": "~0.2.2",
    "rimraf": "~6.1.3",
    "supertest": "~7.2.2",
    "tslib": "~2.8.1",
    "typedoc": "~0.28.17",
    "typedoc-plugin-markdown": "~4.10.0",
    "typescript": "~5.9.3",
    "vitest": "~4.0.18"
  },
  "packageManager": "yarn@4.12.0",
  "volta": {
    "node": "22.3.0",
    "yarn": "1.22.22"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "readme": "README.md"
}
