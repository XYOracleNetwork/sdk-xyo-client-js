{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.73.4",
  "description": "XYO Express Node Server",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "package-compile": "tsup && publint",
    "package-recompile": "tsup && publint"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.413.0",
    "@xylabs/assert": "^2.10.18",
    "@xylabs/sdk-api-express-ecs": "^1.4.0",
    "@xyo-network/express-node-dependencies": "workspace:~",
    "@xyo-network/express-node-middleware": "workspace:~",
    "@xyo-network/express-node-routes": "workspace:~",
    "@xyo-network/node-core-model": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-memory": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/shared": "workspace:~",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "nconf": "^0.12.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "4.17.17",
    "@types/nconf": "^0.10.3",
    "@xylabs/ts-scripts-yarn3": "^2.19.12",
    "@xylabs/tsconfig": "^2.19.12",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
