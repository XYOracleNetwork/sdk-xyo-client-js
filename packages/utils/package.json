{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.6.6",
    "@metamask/providers": "^8.1.1",
    "@xylabs/sdk-js": "^2.5.5",
    "@xyo-network/api": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/domain": "workspace:^",
    "@xyo-network/systeminfo-witness": "workspace:^",
    "@xyo-network/witnesses": "workspace:^",
    "ajv": "^8.11.0",
    "lru-cache": "^7.10.1"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@types/lru-cache": "^7.10.10",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "license-checker": "^25.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "browser": "dist/esm/browserIndex.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/nodeIndex.js",
        "require": "./dist/cjs/nodeIndex.js"
      },
      "browser": {
        "import": "./dist/esm/browserIndex.js",
        "require": "./dist/cjs/browserIndex.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0-only",
  "name": "@xyo-network/utils",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "scripts": {
    "deploy": "yarn deploy3",
    "lint-pkg": "npmPkgJsonLint .",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && yarn gen:docs",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.json",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": true,
  "types": "dist/esm/index.d.ts",
  "version": "2.20.29",
  "packageManager": "yarn@3.1.1"
}
