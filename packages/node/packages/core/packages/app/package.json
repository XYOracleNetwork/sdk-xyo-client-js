{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.71.0",
  "description": "XYO Node App",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.386.0",
    "@xylabs/assert": "^2.9.3",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/account": "workspace:~",
    "@xyo-network/account-model": "workspace:~",
    "@xyo-network/express-node-server": "workspace:~",
    "@xyo-network/node": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/payload-validator": "workspace:~",
    "@xyo-network/schema-name-validator": "workspace:~",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.19.0",
    "@xylabs/tsconfig": "^2.19.0",
    "typescript": "^5.1.6"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "types": "dist/types/index.d.ts"
}
