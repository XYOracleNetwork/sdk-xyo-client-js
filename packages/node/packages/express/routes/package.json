{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-routes",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.40.4",
  "description": "XYO Express Node Routes",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/assert": "^2.6.9",
    "@xylabs/exists": "^2.6.9",
    "@xylabs/sdk-api-express-ecs": "^1.2.5",
    "@xyo-network/api": "workspace:^",
    "@xyo-network/archivist": "workspace:^",
    "@xyo-network/boundwitness": "workspace:^",
    "@xyo-network/core": "workspace:^",
    "@xyo-network/diviner": "workspace:^",
    "@xyo-network/domain-payload-plugin": "workspace:^",
    "@xyo-network/express-node-lib": "workspace:^",
    "@xyo-network/express-node-middleware": "workspace:^",
    "@xyo-network/module": "workspace:^",
    "@xyo-network/node-core-lib": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/payload": "workspace:^",
    "@xyo-network/utils": "workspace:^",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "axios": "*",
    "pako": "*"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/source-map-support": "0.5.6",
    "@types/uuid": "^8.3.4"
  },
  "types": "dist/esm/index.d.ts"
}
