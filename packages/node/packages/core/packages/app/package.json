{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.79.5",
  "description": "XYO Node App",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "exports": {
    ".": {
      "node": {
        "require": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.js"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./app": {
      "node": {
        "require": {
          "types": "./dist/node/app.d.ts",
          "default": "./dist/node/app.js"
        },
        "import": {
          "types": "./dist/node/app.d.mts",
          "default": "./dist/node/app.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.451.0",
    "@xylabs/assert": "^2.13.8",
    "@xylabs/sdk-api-express-ecs": "^1.4.0",
    "@xyo-network/account": "workspace:~",
    "@xyo-network/account-model": "workspace:~",
    "@xyo-network/express-node-server": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-memory": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/payload-validator": "workspace:~",
    "@xyo-network/schema-name-validator": "workspace:~",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.1.21",
    "@xylabs/tsconfig": "^3.1.21",
    "typescript": "^5.2.2"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "types": "dist/node/index.d.ts"
}
