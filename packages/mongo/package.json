{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.73.4",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "scripts": {
    "package-compile": "tsup && publint",
    "package-recompile": "tsup && publint"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/assert": "^2.11.3",
    "@xylabs/delay": "^2.11.3",
    "@xylabs/forget": "^2.11.3",
    "async-mutex": "^0.4.0",
    "mongodb": "^5.9.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^2.19.12",
    "@xylabs/tsconfig": "^3.0.11",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.1.1"
}
