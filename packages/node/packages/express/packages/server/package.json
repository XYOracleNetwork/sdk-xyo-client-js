{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.81.2",
  "description": "XYO Express Node Server",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.js"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.454.0",
    "@xylabs/assert": "^2.13.9",
    "@xylabs/logger": "^2.13.9",
    "@xylabs/sdk-api-express-ecs": "^1.4.0",
    "@xyo-network/express-node-dependencies": "workspace:~",
    "@xyo-network/express-node-middleware": "workspace:~",
    "@xyo-network/express-node-routes": "workspace:~",
    "@xyo-network/node-core-model": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-memory": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "nconf": "^0.12.1",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "4.17.21",
    "@types/nconf": "^0.10.6",
    "@xylabs/ts-scripts-yarn3": "^3.2.8",
    "@xylabs/tsconfig": "^3.2.8",
    "typescript": "^5.3.2"
  }
}
