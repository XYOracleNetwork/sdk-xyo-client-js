{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-middleware",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.43.30",
  "description": "XYO Express Node Middleware",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.266.1",
    "@ethersproject/wallet": "^5.7.0",
    "@xylabs/assert": "^2.7.1",
    "@xylabs/exists": "^2.7.1",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/boundwitness-model": "workspace:^",
    "@xyo-network/express-node-lib": "workspace:^",
    "@xyo-network/modules": "workspace:^",
    "@xyo-network/node-core-lib": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/payload-model": "workspace:^",
    "@xyo-network/shared": "workspace:^",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "node-cache": "^5.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.26.1",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.23.0",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/express-mung": "^0.5.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/source-map-support": "0.5.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@xylabs/ts-scripts-yarn3": "^2.13.11",
    "@xylabs/tsconfig": "^2.13.11",
    "typescript": "^4.9.5"
  },
  "types": "dist/types/index.d.ts"
}
