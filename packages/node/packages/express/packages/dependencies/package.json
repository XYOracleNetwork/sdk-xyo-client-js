{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-dependencies",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.73.4",
  "description": "XYO Express Node Dependencies",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "package-compile": "tsup && publint",
    "package-recompile": "tsup && publint"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.414.0",
    "@xylabs/assert": "^2.11.3",
    "@xylabs/exists": "^2.11.3",
    "@xylabs/sdk-api-express-ecs": "^1.4.0",
    "@xyo-network/account": "workspace:~",
    "@xyo-network/archivist": "workspace:~",
    "@xyo-network/archivist-model": "workspace:~",
    "@xyo-network/core": "workspace:~",
    "@xyo-network/crypto-nft-collection-diviner-score-plugin": "workspace:~",
    "@xyo-network/crypto-nft-collection-payload-plugin": "workspace:~",
    "@xyo-network/crypto-nft-collection-witness-plugin": "workspace:~",
    "@xyo-network/crypto-nft-diviner-score-plugin": "workspace:~",
    "@xyo-network/crypto-nft-payload-plugin": "workspace:~",
    "@xyo-network/crypto-nft-witness-wallet-plugin": "workspace:~",
    "@xyo-network/diviner-address-history": "workspace:~",
    "@xyo-network/diviner-address-space": "workspace:~",
    "@xyo-network/diviner-boundwitness": "workspace:~",
    "@xyo-network/diviner-boundwitness-stats": "workspace:~",
    "@xyo-network/diviner-forecasting": "workspace:~",
    "@xyo-network/diviner-model": "workspace:~",
    "@xyo-network/diviner-payload": "workspace:~",
    "@xyo-network/diviner-payload-stats": "workspace:~",
    "@xyo-network/diviner-schema-list": "workspace:~",
    "@xyo-network/diviner-schema-stats": "workspace:~",
    "@xyo-network/image-thumbnail-plugin": "workspace:~",
    "@xyo-network/manifest": "workspace:~",
    "@xyo-network/module-model": "workspace:~",
    "@xyo-network/node-core-modules-mongo": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/node-model": "workspace:~",
    "@xyo-network/prometheus-node-plugin": "workspace:~",
    "@xyo-network/sentinel": "workspace:~",
    "@xyo-network/url-payload-plugin": "workspace:~",
    "@xyo-network/witness-model": "workspace:~",
    "@xyo-network/witness-timestamp": "workspace:~",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.0.11",
    "@xylabs/tsconfig": "^2.19.12",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
