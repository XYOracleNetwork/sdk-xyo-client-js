{
  "$schema": "https://raw.githubusercontent.com/XYOracleNetwork/sdk-xyo-client-js/main/packages/manifest/src/schema.json",
  "nodes": [
    {
      "config": {
        "name": "DivinerTest",
        "schema": "network.xyo.node.config",
        "language": "javascript"
      },
      "modules": {
        "public": [
          {
            "config": {
              "name": "Archivist",
              "schema": "network.xyo.archivist.config"
            }
          },
          {
            "config": {
              "archivist": "Archivist",
              "name": "BoundWitnessDiviner",
              "schema": "network.xyo.diviner.boundwitness.config"
            }
          },
          {
            "config": {
              "archivist": "Archivist",
              "name": "PayloadDiviner",
              "schema": "network.xyo.diviner.payload.config"
            }
          },
          {
            "config": {
              "archivist": "Archivist",
              "name": "AdHocWitness",
              "schema": "network.xyo.witness.adhoc.config",
              "payload": { "id": 1, "schema": "network.xyo.id" }
            }
          },
          {
            "config": {
              "archiving": {
                "archivists": ["Archivist"]
              },
              "automations": [
                {
                  "frequency": 1,
                  "frequencyUnits": "second",
                  "schema": "network.xyo.automation.interval",
                  "start": 0,
                  "type": "interval"
                }
              ],
              "name": "Sentinel",
              "schema": "network.xyo.sentinel.config",
              "synchronous": "true",
              "tasks": [
                {
                  "input": true,
                  "module": "AdHocWitness"
                }
              ]
            }
          }
        ]
      }
    }
  ],
  "schema": "network.xyo.manifest"
}
